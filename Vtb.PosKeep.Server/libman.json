{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "vue@2.6.10",
      "destination": "wwwroot/lib/vue/"
    },
    {
      "library": "axios@0.19.0",
      "destination": "wwwroot/lib/axios/"
    },
    {
      "library": "vue-router@3.1.3",
      "destination": "wwwroot/lib/vue-router/"
    },
    {
      "library": "apexcharts@3.8.6",
      "destination": "wwwroot/lib/apexcharts/"
    },
    {
      "provider": "filesystem",
      "library": "node_modules/vue-apexcharts/dist/",
      "destination": "wwwroot/lib/vue-apexcharts",
      "files": [
        "vue-apexcharts.js"
      ]
    },
    {
      "library": "vuejs-datepicker@1.6.2",
      "destination": "wwwroot/lib/vuejs-datepicker/"    
    }
